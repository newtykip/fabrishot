{
  "schemaVersion": 1,
  "id": "fabrishot",
  "version": "${version}",
  "icon": "assets/fabrishot/icon.png",
  "environment": "client",
  "mixins": [
    "mixins.fabrishot.json"
  ],
  "entrypoints": {
    "client": [
      "dev.newty.fabrishot.Fabrishot"
    ],
    "modmenu": [
      "dev.newty.fabrishot.config.ModMenuBridge"
    ]
  },
  "name": "Fabrishot",
  "description": "Take high resolution screenshots because why not",
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabric-lifecycle-events-v1": "*",
    "fabric-key-binding-api-v1": "*"
  },
  "recommends": {
    "fabric-resource-loader-v0": "*",
    "modmenu": ">=${modmenu_version}",
    "yet_another_config_lib_v3": ">=${yacl_version}"
  },
  "authors": [
    "newtykip",
    "ramidzkh"
  ],
  "license": [
    "MIT"
  ],
  "contact": {
    "homepage": "https://github.com/newtykip/fabrishot#readme",
    "issues": "https://github.com/newtykip/fabrishot/issues",
    "sources": "https://github.com/newtykip/fabrishot"
  }
}
